{
  "name": "timoneer",
  "version": "0.0.2",
  "homepage": "https://github.com/leonardochaia/timoneer",
  "repository": "https://github.com/leonardochaia/timoneer",
  "description": "Docker Client",
  "author": {
    "name": "Leonardo Chaia",
    "email": "lchaia@astonishinglab.com"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "yarn postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web && yarn gen-ver",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "gen-ver": "node tim-version-gen",
    "start": "yarn postinstall:electron && yarn gen-ver && npm-run-all -p ng:serve electron:serve",
    "build": "yarn postinstall:electron && yarn gen-ver && yarn electron:tsc && ng build",
    "build:dev": "yarn build -c dev",
    "build:prod": "yarn build -c production",
    "ng:serve": "ng serve --configuration hmr",
    "ng:serve:web": "yarn postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts --lib es2017,es2016,es2015,dom",
    "electron:serve": "wait-on http-get://localhost:4200/ && yarn electron:tsc && electron . --serve",
    "electron:local": "yarn build:prod && electron .",
    "electron:linux": "yarn build:prod && npx electron-builder build --linux",
    "electron:windows": "yarn build:prod && npx electron-builder build --windows",
    "electron:mac": "yarn build:prod && npx electron-builder build --mac",
    "test": "yarn postinstall:web && ng test",
    "e2e": "yarn postinstall:web && ng e2e",
    "electron:build": "yarn build:prod && npx electron-builder build --linux --windows",
    "release": "yarn build:prod && electron-builder"
  },
  "dependencies": {
    "electron-settings": "^3.2.0",
    "electron-updater": "^3.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.1",
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/cli": "6.1.1",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@angular/material": "^6.4.1",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/dockerode": "^2.5.5",
    "@types/electron-settings": "^3.1.1",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.4",
    "angular-pipes": "^8.0.0",
    "angular-split-ng6": "^1.0.0-rc.5",
    "codelyzer": "4.4.2",
    "core-js": "2.5.7",
    "dockerode": "^2.5.5",
    "electron": "2.0.5",
    "electron-builder": "20.26.1",
    "electron-reload": "1.2.5",
    "fast-text-encoding": "^1.0.0",
    "git-rev-sync": "^1.12.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "path": "^0.12.7",
    "protractor": "5.4.0",
    "rxjs": "6.2.2",
    "split-ca": "^1.0.1",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.1.0",
    "xterm": "^3.5.1",
    "zone.js": "0.8.26"
  }
}
